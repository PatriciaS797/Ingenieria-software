        android:theme="@style/Theme.HotelRural">
        <activity
            android:name="es.unizar.eina.hotel.gestion.Menu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".habitaciones.Hab">
